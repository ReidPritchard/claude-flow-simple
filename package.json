{
  "name": "claude-flow",
  "version": "2.0.0-alpha.90",
  "description": "Simplified AI agent orchestration CLI tool",
  "main": "cli.mjs",
  "bin": {
    "claude-flow": "bin/claude-flow.js"
  },
  "scripts": {
    "dev": "tsx src/cli/main.ts",
    "build": "npm run clean && npm run build:esm",
    "clean": "rm -rf dist",
    "build:esm": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "node src/cli/main.js"
  },
  "keywords": [
    "claude",
    "ai",
    "agent",
    "orchestration",
    "mcp",
    "workflow",
    "automation",
    "swarm",
    "ruv-swarm",
    "github",
    "docker",
    "enterprise",
    "coordination",
    "multi-agent",
    "neural-networks",
    "cli",
    "tools",
    "alpha"
  ],
  "author": "rUv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruvnet/claude-code-flow.git"
  },
  "bugs": {
    "url": "https://github.com/ruvnet/claude-code-flow/issues"
  },
  "homepage": "https://github.com/ruvnet/claude-code-flow#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "cli.js",
    "bin/claude-flow.js",
    "bin/claude-flow",
    "bin/claude-flow-dev",
    "bin/claude-flow-pkg.js",
    "bin/claude-flow-swarm",
    "bin/claude-flow-swarm-background",
    "bin/claude-flow-swarm-bg",
    "bin/claude-flow-swarm-monitor",
    "bin/claude-flow-swarm-ui",
    "dist/",
    "src/",
    ".claude/",
    "docker-test/",
    "scripts/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "DOCKER_TEST_REPORT.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "type": "module"
}
